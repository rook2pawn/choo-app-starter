  "description": "",
  "main": "index.js",
  "dependencies": {
    "browserify": "^17.0.0",
    "choo": "^7.1.0",
    "choo-devtools": "^3.0.4",
    "http-server": "^13.0.1",
    "nanocomponent": "^6.6.0",
    "nanohtml": "^1.9.1",
    "nanostate": "^1.2.1",
    "sheetify": "^8.0.0",
    "watchify": "^4.0.0"
  },
  "scripts": {
    "start-spa": "http-server ./web -c-1 -p 5050 --proxy http://localhost:5050?",
    "start-proxy-secure": "node proxy-secure.js",
    "start-proxy": "node proxy.js -p 8080",
    "start-backend": "node server.js -p 5150 --unhandled-rejections=strict",
    "start-dev": "http-server ./web -o -c-1 -p 8081 --proxy http://localhost:8081?",
    "watch": "watchify -v -g sheetify ./app/index.js -o ./web/bundle.js",
    "build": "browserify -v -g sheetify ./app/index.js -o ./web/bundle.js"
